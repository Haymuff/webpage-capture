  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-xo-space": "^0.18.0",
    "jest": "^22.4.3",
    "nsp": "^3.2.1",
    "regenerator-runtime": "^0.12.1"
  },
  "scripts": {
    "prepublish": "nsp check --warn-only",
    "pretest": "eslint . --fix",
    "test": "jest --coverage",
    "start": "babel-node lib/index.js"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "no-await-in-loop": 0
    }
  },
  "repository": "b4dnewz/webpage-capture",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.15.1",
    "lodash": "^4.17.11",
    "puppeteer": "^1.8.0"
