{
  "name": "webpage-capture",
  "version": "0.2.0",
  "description": "Capture the web using headless chrome",
  "homepage": "https://github.com/b4dnewz/webpage-capture",
  "author": "Filippo Conti <filippo@codekraft.it>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "bin": {
    "webcapture": "dist/cli.js"
  },
  "keywords": [
    "webpage",
    "screenshots"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "prebuild": "npm run test",
    "lint": "eslint . --fix",
    "test": "jest --coverage",
    "start": "babel-node lib/index.js",
    "dev": "babel lib --watch --out-dir dist",
    "build": "babel lib --out-dir dist"
  },
  "repository": "b4dnewz/webpage-capture",
  "dependencies": {
    "commander": "^2.19.0",
    "lodash": "^4.17.11",
    "ora": "^3.1.0",
    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "eslint": "^5.14.1",
    "eslint-config-xo": "^0.26.0",
    "jest": "^24.1.0",
    "regenerator-runtime": "^0.13.1"
  },
  "eslintConfig": {
    "extends": "xo",
    "env": {
      "jest": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "no-await-in-loop": 0,
      "valid-jsdoc": 0,
      "capitalized-comments": 0
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
